{
  "compilerOptions": {
    "strict": true,
    "alwaysStrict": true,
    "esModuleInterop": true,
    "module": "CommonJS",
    "downlevelIteration": true,
    "target": "ES2022",
    "moduleResolution": "Node",
    "strictNullChecks": true,
    "sourceMap": true,
    "outDir": "build"
  },
  "exclude": ["./build"]
}
